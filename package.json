{
  "name": "nixify",
  "private": false,
  "version": "0.0.9",
  "type": "module",
  "scripts": {
    "build": "bun ./scripts/build.js",
    "build:watch": "bun --watch ./scripts/build.js",
    "dev": "bun --watch src/index.js",
    "clean": "rm -rf dist",
    "prebuild": "bun run clean"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/bun": "^1.3.3",
    "vite": "^7.2.2"
  },
  "dependencies": {
    "@vue/reactivity": "^3.5.25"
  }
}
